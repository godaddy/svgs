{
  "name": "svgs",
  "version": "3.1.2",
  "description": "svgs is a compatiblity layer between svg and react-native-svg",
  "main": "./lib",
  "react-native": "./index",
  "scripts": {
    "test": "mocha --require enzyme/withDom --compilers js:babel-register test.js",
    "lint": "eslint-godaddy-react index.js index.native.js test.js",
    "build": "babel ./index.js -d ./lib",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godaddy/svgs"
  },
  "mochify": {
    "transform": "babelify",
    "reporter": "spec",
    "ui": "bdd"
  },
  "keywords": [
    "svg",
    "react-native",
    "svgs",
    "compatiblity",
    "element",
    "react"
  ],
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "prop-types": "^15.5.10",
    "rip-out": "^1.0.0"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "assume": "^1.5.0",
    "babel-cli": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.8.0",
    "eslint": "^3.15.0",
    "eslint-config-godaddy-react": "^1.1.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-react": "^6.9.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.3",
    "react-native-svg": "^5.1.7"
  },
  "author": "GoDaddy Operating Company, LLC",
  "contributors": [
    "Arnout Kazemier"
  ],
  "license": "MIT",
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
