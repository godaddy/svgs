{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js -t babelify --outfile bundle.js"
  },
  "dependencies": {
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "react": "16.5.0",
    "react-dom": "16.5.0"
  },
  "babel": {
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
